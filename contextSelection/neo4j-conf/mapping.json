{
  "defaults": {
    "key_property": "id",
    "nodes_index": "default-index-node",
    "relationships_index": "default-index-relationship",
    "include_remaining_properties": true
  },
  "node_mappings": [
    {
      "condition": "allNodes()",
      "type": "nodes"
    }
  ],
  "relationship_mappings": [
    {
      "condition": "allRelationships()",
      "type": "getType()"
    }
  ]
}